{
  "name": "jam",
  "version": "0.3.1",
  "description": "jam! experimental version",
  "main": "index.js",
  "scripts": {
    "mocha": "mocha --compilers js:babel-core/register src/**/*.spec.js test src/api.spec.js",
    "test": "npm run mocha",
    "test-browser": "testem",
    "start": "NODE_ENV=dev node server.js",
    "build": "NODE_ENV=production webpack -p",
    "profile": "webpack --profile --json > stats.json",
    "release": "npm run release-patch",
    "release-patch": "git checkout master ; npm version patch && git push origin master --tags ",
    "release-minor": "git checkout master ; npm version patch && git push origin master --tags ",
    "release-major": "git checkout master ; npm version patch && git push origin master --tags ",
    "make-offline_simple": "babel src/components/webgl/view.js --out-file dist/jam-headless.js",
    "make-offline-rollup": "rollup src/components/webgl/view.js > dist/jam-headless.js",
    "make-offline-webpack": "webpack --config webpack.config.headless.js",
    "make-offline": "browserify src/components/webgl/view.js -t babelify --outfile dist/jam-headless.js"
  },
  "author": "Mark 'kaosat-dev' Moissette",
  "license": "AGPL-3.0",
  "repository": "usco/Jam",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "compression-webpack-plugin": "^0.1.2",
    "css-loader": "^0.23.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.1",
    "mocha": "^2.2.5",
    "react-hot-loader": "^1.2.2",
    "rollup": "^0.21.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "@cycle/core": "^5.0.1",
    "@cycle/dom": "^7.1.1",
    "addressbar": "^0.4.3",
    "classnames": "^2.2.1",
    "fast.js": "^0.1.1",
    "gl": "^2.1.5",
    "glView-helpers": "usco/glView-helpers",
    "jsondiffpatch": "^0.1.31",
    "log-minim": "usco/log-minim",
    "loglevel": "^1.2.0",
    "pngjs": "^2.2.0",
    "ramda": "^0.18.0",
    "react": "^0.13.3",
    "rx": "^4.0.7",
    "rx-dom": "^5.0.3",
    "screenfull": "^3.0.0",
    "seamless-immutable": "^4.0.1",
    "three": "^0.70.0",
    "tween.js": "^0.14.0",
    "usco-obj-parser": "usco/usco-obj-parser",
    "usco-stl-parser": "usco/usco-stl-parser",
    "viewport-size": "0.0.1"
  }
}
